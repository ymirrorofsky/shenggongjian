.calender {

                width:1000px;
                position:relative;
                margin:0 auto;
}
.calender .imgTitle
{
    position:absolute;
    top:-45px;
    left:0;
    z-index:1;
    width:1000px;
    height:45px;
    text-align:left;
    overflow:hidden;
}
.calender  .imgTitle .bg {
    background: #252525 none repeat scroll 0 0;
    height: 45px;
    opacity: 0.5;
    ilter:alpha(opacity=50);
    position: absolute;
    top: 0;
    width: 100%;
    z-index: -1;
}


.calender .imgTitle .day
{
    position:absolute;
    left:0px;
    width:114px;
    height:45px;
    background: #01164b;
    text-align:center;
    line-height:50px;
    font-size:15px;
    color:#ffc600;
}
.calender  .imgTitle .day.today
{
    font-size:24px;
}

.calender .month
{
     height:38px;
    background: #01164b;
    position:relative;
}

.calender .month .arrow
{

    position:absolute;
    color:#fff;
    font-size:20px;
    line-height:38px;
    padding:0 3px;
    *width:28px;
    cursor:pointer;
}
.calender .month .arrowleft
{

    left:2px;
    top:10px;
    width:10px;
    height:15px;
    background: url("../images/tb1.png") no-repeat;
}

.calender .month .arrowright
{
    right:2px;
    top:1px;
    width:10px;
    height:15px;
    background: url("../images/tb2.png") no-repeat;
}


.calender .month .monthTxt
{
    float:left;
    _display:inline;
    margin:0 15px 0 40px;
    color:#9884ab;
}

#calendarYear {display:none;}

.calender .month .monthTxt span
{
    font-size:20px;
    line-height:38px;
    padding-right:3px;
    color:#d7d0dd;
}


.calender .month .day
{
    float:left;
    _display:inline;
    margin:0;
    padding-left:10px;
}
.calender .month .day li {
    float: left;
    font-size: 12px;
    padding: 4px 2px;
}
.calender.month ul, li {
    list-style-type: none;
}
.calender .month .day a
{
    color:#fff;
    display:block;
    line-height:30px;
    text-align:center;
    width:24px;
    cursor:pointer;
    zoom:1;
}


.calender .month .day a:hover
{
    background:#703d97;
}

.calender .month .day a.none
{
    color:#866c9c;
}

.calender .month .day a.none:hover
{
    background:none;
    cursor:text;
}

.calender .month .day a.current
{
    color:#ffffff;
}
